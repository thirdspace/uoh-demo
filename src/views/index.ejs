                  "chart": {
                      "caption": "Skills",
                      "subCaption": "",                                                                                               
                      "numberPreffix": "$",
                      "theme": "carbon",
                      "radarfillcolor": "#ffffff",
                  },
                  "categories": [
                      {
                          "category": [
