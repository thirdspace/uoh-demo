       <!-- Swiper JS -->
    <script src="/js/plugins/swiper/swiper.min.js"></script>

    <!-- Initialize Swiper -->
    <script>
    var swiper_JdSpiderCharts = new Swiper('.swiper-container', {
        pagination: '.swiper-pagination',
        effect: 'coverflow',
        grabCursor: false,
        centeredSlides: true,
        slidesPerView: 'auto',
        coverflow: {
            rotate: 50,
            stretch: 0,
            depth: 100,
            modifier: 1,
            slideShadows : true
        }
    });
    </script>

